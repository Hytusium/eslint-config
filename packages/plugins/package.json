    "eslint-plugin-unicorn": "52.0.0",
    "eslint-plugin-unused-imports": "3.2.0",
    "typescript-eslint": "7.8.0"
  },
  "devDependencies": {
    "@types/node": "20.12.11",
    "typescript": "5.4.5"
  },
  "peerDependencies": {
