{
  "private": true,
  "name": "@hytusium/eslint-plugins",
  "version": "0.1.0",
  "description": "eslint plugins for personal (kash / Hytus) projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/Hytusium/eslint-config.git",
    "directory": "packages/plugins"
  },
  "author": "kash / Hytus <kash@hytus.net>",
